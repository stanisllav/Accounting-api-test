        "/api/transactions": {
            "get": {
                "tags": [
                    "Transactions"
                ],
                "summary": "Returns a paginated set of Transactions",
                "operationId": "69ed256f7b59b0683aee4b2298765d16",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "name": "amount",
                        "in": "query"
                    },
                    {
                        "name": "date",
                        "in": "query"
                    },
                    {
                        "name": "type",
                        "in": "query",
                        "schema": {
                            "enum": [
                                "income",
                                "outcome"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "A paginated set of Transactions",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "schema": "array",
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Transaction"
                                            }
                                        },
                                        "current_page": {
                                            "type": "integer"
                                        },
                                        "next_page_url": {
                                            "type": "string"
                                        },
                                        "path": {
                                            "type": "string"
                                        },
                                        "per_page": {
                                            "type": "integer"
                                        },
                                        "prev_page_url": {
                                            "type": "string"
                                        },
                                        "to": {
                                            "type": "integer"
                                        },
                                        "total": {
                                            "type": "integer"
                                        },
                                        "first_page_url": {
                                            "type": "string"
                                        },
                                        "from": {
                                            "type": "integer"
                                        },
                                        "last_page": {
                                            "type": "string"
                                        },
                                        "last_page_url": {
                                            "type": "string"
                                        },
                                        "links": {
                                            "type": "array",
                                            "items": {
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            },
